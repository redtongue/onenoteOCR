<?xml version="1.0"?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>

    <appSettings>
        <add key="tmpPath" value="tmp"/>
        
        <!-- 线程休眠时间，单位毫秒，若图片很大，则延长休眠时间，保证Onenote OCR完毕 -->
        <add key="WaitTIme" value="200"/>
    </appSettings>
</configuration>
